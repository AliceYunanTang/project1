<div class="container">
  <div class="row">
    <div class="col-md-6 col-md-offset-3 text-center">

      <h2>Users Index</h2>
      <table class="table table-striped">

        <% @all_users.each do |u| %>
        <tr>
          <td>
            <%= link_to u.name, user_path(u.id) %>
          </td>
          <td>
            <%= u.email %>
          </td>
        </tr>
        <% end %>

      </table>

    </div>
  </div>
</div>
