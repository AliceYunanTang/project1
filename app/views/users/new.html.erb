<div class="container">
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <div class="page-header">
        <h2>Create Account</h2>
      </div>
      <% if @user.errors.any? %>
      <ol>
        <% @user.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
        <% end %>
      </ol>
      <% end %>
    </div>
  </div>
</div>
<%= render partial: 'form' %>
