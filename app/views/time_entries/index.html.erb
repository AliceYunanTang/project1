<h2>Time Entries Index</h2>
<ul>
<% @entries.each do |project, entry| %>
  <li>
    <strong><%= project.name %></strong>

    <ul>
    <% entry.each do |e| %>
      <li> <%= e.user.name %> <%= e.start_time.strftime "%m:%M %p %a %b %Y"  %> to <%= e.end_time.strftime "%m:%M %p %a %b %Y" %></li>
    <% end %>
    </ul>

    <%#= link_to time_entry.user.name, time_entry_path(time_entry.id) %>
  </li>
<% end %>
</ul>
