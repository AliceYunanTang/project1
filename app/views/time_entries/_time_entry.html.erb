<div class="row">
    <div class="col-md-1"><div class="mini-box"><%= time_entry.user.name %></div></div>
    <div class="col-md-2"><div class="mini-box"><%= time_entry.project.name %></div></div>
    <div class="col-md-2"><div class="mini-box"><%= time_entry.start_time %></div></div>
    <div class="col-md-2"><div class="mini-box"><%= time_entry.end_time %></div></div>
    <div class="col-md-1"><div class="mini-box"><%= time_entry.time %></div></div>
    <div class="col-md-1"><div class="mini-box"><%= time_entry.amount %></div></div>
    <div class="col-md-1"><div class="mini-box"><%= link_to 'Edit', edit_time_entry_path(time_entry.id) %></div></div>
    <div class="col-md-1"><div class="mini-box"><%= link_to 'Delete', time_entry_path(time_entry.id), method: :delete, data: {confirm: 'Are you sure?'} %></div></div>
</div>
